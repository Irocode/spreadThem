{
  "name": "SpreadThem!",
  "version": "0",
  "description": "Convert tables to spreadsheets!",
  "background": {"page": "background.html"},
  "manifest_version": 2,
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "content_scripts": [ {
    "js": ["jquery.js", "handsOnTable.js", "content.js"],
    "css": ["handsOnTable.css", "customStyles.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
